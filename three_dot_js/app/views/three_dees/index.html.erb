<p id="notice"><%= notice %></p>

<h1>Listing Three Dees</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @three_dees.each do |three_dee| %>
      <tr>
        <td><%= three_dee.name %></td>
        <td><%= three_dee.description %></td>
        <td><%= link_to 'Show', three_dee %></td>
        <td><%= link_to 'Edit', edit_three_dee_path(three_dee) %></td>
        <td><%= link_to 'Destroy', three_dee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Three dee', new_three_dee_path %>
